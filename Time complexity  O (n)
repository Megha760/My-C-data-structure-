#include <stdio.h>
#include <time.h>

int main() {
    clock_t start = clock();

    int n = 100;
    int result = n * 2;  // single operation

    clock_t end = clock();
    printf("O(1) Example: Result = %d\n", result);
    printf("Time taken: %f seconds\n", (double)(end - start) / CLOCKS_PER_SEC);

    return 0;
}


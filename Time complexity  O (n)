#include <stdio.h>
#include <time.h>

int main() {
    int n = 1000000;
    clock_t start = clock();

    long long sum = 0;
    for(int i = 0; i < n; i++) {
        sum += i;  // loop runs n times
    }

    clock_t end = clock();
    printf("O(n) Example: Sum = %lld\n", sum);
    printf("Time taken: %f seconds\n", (double)(end - start) / CLOCKS_PER_SEC);

    return 0;
}



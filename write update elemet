#include<stdio.h>
int main(){
    int a[]={1,2,3,4};
    int n = sizeof(a)/sizeof(a[0]);
    int newvalue =32;
    int index = 2;
    if(index>=0 && index<n){
        a[index]=newvalue;
        for(int i=0; i<n;i++){
            printf("%d ", a[i]);
        }
    }
    else{
        printf("summne");
    }
}

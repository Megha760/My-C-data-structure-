#include <stdio.h>

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int *ptr1 = &arr[1];  // points to 20
    int *ptr2 = &arr[4];  // points to 50

    int diff = ptr2 - ptr1;  // number of elements between ptr2 and ptr1
    printf("ptr2 points to: %d\n", *ptr2);
    printf("ptr1 points to: %d\n", *ptr1);
    printf("Number of elements between ptr2 and ptr1: %d\n", diff);

    return 0;
}

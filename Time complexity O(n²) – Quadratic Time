#include <stdio.h>
#include <time.h>

int main() {
    int n = 1000;
    clock_t start = clock();

    long long count = 0;
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < n; j++) {
            count++;  // nested loop runs n*n times
        }
    }

    clock_t end = clock();
    printf("O(n^2) Example: Count = %lld\n", count);
    printf("Time taken: %f seconds\n", (double)(end - start) / CLOCKS_PER_SEC);

    return 0;
}

